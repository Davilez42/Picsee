                <a href="">#Dolor</a>
                 <a href="">#Nocher</a>  
                <a href="">#Dark</a>  
                <a href="">#Dance</a>  
                <a href="">#Dolor</a>       
            </div>
            
        </div>

        <div id="contenedor_galeria_perfil">
            <div class="row_galeria_perfil">
                <% let j = 0 %>
                <% let n = imagenes.length %>
                <% for(let i=0;i < 4;i++ ){ %>
                <div class="column_galeria_perfil">
                        <% if(j < n){ %>
                            <% for(let k=0;k < 3;k++ ) {%>
                                
                                    <div class="contenedor_imagen">
                                    <img class="imagen_galeria" src="<%= imagenes[j] %>" alt="" />
                                    <div>
                                        <li class="opciones_imagenes">
                                            <ul><a id="<%= imagenes[j] %>"  class="boton_like"><img class="icono_opciones" src="./icons/corazon_like_desactivado.png" ></a></ul>
                                            <ul><a id="<%= imagenes[j] %>" class="boton_like"><img class="icono_opciones" src="/./icons/icono_compartir.png" ></a></ul>
                                            <ul><a id="<%= imagenes[j] %>" class="boton_like"><img class="icono_opciones" src="/./icons/icono_comentario.png" ></a></ul>
                                        </li>
                                    </div>
                                </div>
                                <% j++ %>

                            <% } %> 
                        <% }%>     
                </div>
                <% } %>

               
                
            </div>

        </div>
        <script src="./js/cargar_configuracion_Perfil.js"></script>
      </main>
    
